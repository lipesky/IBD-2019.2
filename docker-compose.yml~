# Use postgres/example user/password credentials
version: '2'

services:

  db:
    image: postgres
    restart: always
    environment:
       POSTGRES_USER: runed
       POSTGRES_PASSWORD: 123
    volumes:
       - ./db/runeterra.sql:/docker-entrypoint-initdb.d/

  adminer:
    image: adminer
    restart: always
    ports:
      - 8087:8080
      
      
